<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="450.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gomoku.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="427.0" prefWidth="560.0">
      <children>
         <Label ellipsisString="" layoutX="139.0" layoutY="139.0" prefHeight="39.0" prefWidth="283.0" text="Welcome to Gomoku" textAlignment="CENTER">
            <font>
               <Font size="32.0" />
            </font>
         </Label>
         <Label ellipsisString="" layoutX="199.0" layoutY="190.0" prefHeight="28.0" prefWidth="163.0" text="Please enter your name" textAlignment="CENTER">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="nameTxf" layoutX="197.0" layoutY="218.0" prefHeight="26.0" prefWidth="163.0" />
         <Button fx:id="startBtn" layoutX="239.0" layoutY="264.0" mnemonicParsing="false" onAction="#startClick" text="Start Game" />
      </children>
   </AnchorPane>
</VBox>
